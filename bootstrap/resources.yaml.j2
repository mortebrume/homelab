---
apiVersion: v1
kind: Secret
metadata:
  name: bitwarden
  namespace: security
  labels:
    reconcile.external-secrets.io/managed: 'true'
stringData:
  {{ ENV['bitwarden'] | indent(2) | safe }}
---
apiVersion: v1
kind: Secret
metadata:
  name: cloudflare-issuer
  namespace: security
  labels:
    reconcile.external-secrets.io/managed: 'true'
stringData:
  token: {{ ENV['CLOUDFLARE_DNS_TOKEN'] }}
